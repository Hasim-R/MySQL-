mysql> select department, avg(salary), count(gender) from employee group by department having avg(salary)>60000;
+------------+--------------+---------------+
| department | avg(salary)  | count(gender) |
+------------+--------------+---------------+
| Finance    | 60250.000000 |             4 |
| IT         | 71200.000000 |             5 |
+------------+--------------+---------------+
2 rows in set (0.00 sec)


mysql> SELECT gender, department, AVG(salary) AS avg_salary, COUNT(*) AS gender_count
    -> FROM employee
    -> GROUP BY department, gender
    -> HAVING avg_salary > 60000;
+--------+------------+--------------+--------------+
| gender | department | avg_salary   | gender_count |
+--------+------------+--------------+--------------+
| Male   | IT         | 69666.666667 |            3 |
| Female | IT         | 73500.000000 |            2 |
| Female | Finance    | 62000.000000 |            1 |
+--------+------------+--------------+--------------+
3 rows in set (0.00 sec)

mysql> SELECT department, COUNT(*) AS female_count, AVG(salary) AS avg_salary
    -> FROM employee
    -> WHERE gender = 'Female'
    -> GROUP BY department
    -> HAVING avg_salary > 60000;
+------------+--------------+--------------+
| department | female_count | avg_salary   |
+------------+--------------+--------------+
| IT         |            2 | 73500.000000 |
| Finance    |            1 | 62000.000000 |
+------------+--------------+--------------+
2 rows in set (0.00 sec)


