mysql> SELECT department
    -> FROM employee
    -> GROUP BY department
    -> HAVING COUNT(DISTINCT gender) = 2;
+------------+
| department |
+------------+
| Finance    |
| HR         |
| IT         |
| Marketing  |
+------------+
4 rows in set (0.00 sec)


mysql> SELECT department, AVG(experience)
    -> FROM employee
    -> GROUP BY department
    -> ORDER BY AVG(experience) DESC
    -> LIMIT 1;
+------------+-----------------+
| department | AVG(experience) |
+------------+-----------------+
| HR         |         12.6667 |
+------------+-----------------+
1 row in set (0.00 sec)

mysql> SELECT department, AVG(experience)
    -> FROM employee
    -> GROUP BY department
